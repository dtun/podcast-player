{
  "preset": "jest-expo",
  "setupFilesAfterEnv": ["./jest.setupFilesAfterEnv.js"],
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.{js,jsx,ts,tsx}",
    "!**/coverage/**",
    "!**/node_modules/**",
    "!**/babel.config.js",
    "!**/jest.setup.js",
    "!**/.expo/**",
    "!**/mocks/**",
    "!**/test-utils/**",
    "!**/builders/**"
  ]
}
